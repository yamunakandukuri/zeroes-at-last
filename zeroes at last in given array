public class zeroes
{
    public static void main(String args[])
 {
   int[] arr = {35, 2, 1, 0, 4, 0, 0, 2, 7, 0, 12, 0, 6, 8, 9, 0, 9, 1, 0, 8, 5};
   int index = 0;
     for (int i = 0; i < arr.length; i++) 
{
             if (arr[i] != 0)  
 {
         arr[index] = arr[i];
        index++;
    }
   }
 while (index < arr.length) 
{
     arr[index] = 0;
     index++;
 }
 for (int i = 0; i < arr.length; i++)
 {
   System.out.print(arr[i]+" ");
  }
 }
}
